{
    "manifest_version": 3,
    "name": "YouTube Quiet Ads",
    "description": "Hide midroll ads on YouTube.",
    "version": "1.0.0",
    "permissions": [
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
